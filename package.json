{
  "name": "dp-data-table",
  "version": "1.0.4",
  "description": "A simple and easy to customize data table with filter and sort functionalities. This component is build using React and can be directly used in static html as well.",
  "main": "dpDataTable.js",
  "scripts": {
    "test": "eslint lib/ spec/ && ./node_modules/karma/bin/karma start karma.conf.js",
    "watch-test": "./node_modules/karma/bin/karma start karma.conf.js --auto-watch --no-single-run",
    "build": "webpack -p",
    "rename-css-bower-dependencies": "find bower_components -name '*.css' | sed -e 'p;s/.css$/.scss/' | xargs -n2 mv",
    "start": "webpack-dev-server --inline --hot",
    "postinstall": "bower install"
  },
  "author": "daniepaul@gmail.com",
  "license": "MIT",
  "keywords": [
    "react-component",
    "sortable table",
    "data table",
    "sortable data table",
    "filterable data table",
    "filterable table",
    "filterable grid",
    "easy data table",
    "simple data table",
    "dp-data-table"
  ],
  "devDependencies": {
    "babel-core": "~5.8.22",
    "babel-eslint": "^4.0.10",
    "babel-loader": "~5.3.2",
    "babel-polyfill": "^6.23.0",
    "css-loader": "~0.16.0",
    "es5-shim": "~4.1.10",
    "eslint": "^0.21.1",
    "eslint-plugin-react": "^2.3.0",
    "jasmine": "~2.3.2",
    "jasmine-core": "~2.3.4",
    "json-loader": "^0.5.4",
    "karma": "~0.13.9",
    "karma-jasmine": "~0.3.6",
    "karma-phantomjs-launcher": "~0.2.1",
    "karma-sourcemap-loader": "~0.3.5",
    "karma-webpack": "~1.7.0",
    "node-sass": "^4.5.2",
    "phantomjs": "^2.1.7",
    "sass-loader": "~2.0.1",
    "style-loader": "~0.12.3",
    "unexpected": "^10.26.3",
    "unexpected-react": "^3.5.2",
    "webpack": "~1.11.0",
    "webpack-dev-server": "~1.10.1"
  },
  "peerDependencies": {
    "babel-runtime": "~5.8.20",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "react": "~15.0.0",
    "react-dom": "~15.0.0"
  },
  "repository": "https://github.com/daniepaul/dp-data-table.git"
}
