{
  "name": "dp-data-table",
  "version": "1.1.0",
  "description": "A simple and easy to customize data table with filter and sort functionalities. This component is build using React and can be directly used in static html as well.",
  "main": "lib/dpDataTable.js",
  "unpkg": "dist/dpDataTable.js",
  "scripts": {
    "test": "eslint src/ && ./node_modules/karma/bin/karma start karma.conf.js",
    "watch-test": "./node_modules/karma/bin/karma start karma.conf.js --auto-watch --no-single-run",
    "build": "npm run build-dist && npm run build-lib",
    "build-dist": "set arg=dist && webpack --env.ships=dist -p",
    "build-lib": "set arg=lib && webpack --env.ships=lib -p",
    "start": "webpack-dev-server --inline --hot",
    "prepublish": "npm run test && npm run build"
  },
  "author": "daniepaul@gmail.com",
  "license": "MIT",
  "keywords": [
    "react-component",
    "sortable table",
    "data table",
    "sortable data table",
    "filterable data table",
    "filterable table",
    "filterable grid",
    "easy data table",
    "simple data table",
    "dp-data-table"
  ],
  "devDependencies": {
    "babel-core": "~6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "~7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "~0.28.7",
    "es5-shim": "~4.5.9",
    "eslint": "^4.6.1",
    "eslint-plugin-react": "^7.3.0",
    "jasmine": "~2.8.0",
    "jasmine-core": "~2.8.0",
    "json-loader": "^0.5.7",
    "karma": "~1.7.1",
    "karma-jasmine": "~1.1.0",
    "karma-phantomjs-launcher": "~1.0.4",
    "karma-sourcemap-loader": "~0.3.5",
    "karma-webpack": "~2.0.4",
    "node-sass": "^4.5.3",
    "phantomjs": "^2.1.7",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "~6.0.6",
    "style-loader": "~0.18.2",
    "unexpected": "^10.35.0",
    "unexpected-react": "^4.1.0",
    "webpack": "~3.5.5",
    "webpack-dev-server": "~2.7.1"
  },
  "peerDependencies": {
    "babel-runtime": "~6.26.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "react": "~15.6.1",
    "react-dom": "~15.6.1",
    "prop-types": "^15.5.10"
  },
  "repository": "https://github.com/daniepaul/dp-data-table.git"
}
